    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <FrameLayout
        android:layout_width="0px"
        android:layout_height="match_parent"
        android:layout_weight="3.5"
        android:layout_marginRight="15dp"
        android:id="@+id/musicians_list_frame"></FrameLayout>
    <FrameLayout
        android:layout_width="0px"
        android:layout_height="match_parent"
